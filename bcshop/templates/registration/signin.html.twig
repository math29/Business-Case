{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}

    <h1>Connexion</h1>

    <form action="{{ path('signin') }}" method="post">
        <input type="text" name="_email" id="" placeholder="email">
        <input type="password" name="_password" id="" placeholder="password">

        <button type="submit" class="btn btn-success">Sign in</button>
    </form>
{% endblock %}
